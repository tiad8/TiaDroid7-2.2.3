J               'J'     '5'     'j'     'J'     '8'     '8'
K               'K'     '5'     'k'     'K'     '9'     '9'
L               'L'     '5'     'l'     'L'     '|'     '|'
M               'M'     '6'     'm'     'M'     '?'     '?'
N               'N'     '6'     'n'     'N'     '0'     '0'
O               'O'     '6'     'o'     'O'     '+'     '+'
P               'P'     '7'     'p'     'P'     '-'     '-'
Q               'Q'     '7'     'q'     'Q'     '`'     '`'
R               'R'     '7'     'r'     'R'     0xA3    0xA3
S               'S'     '7'     's'     'S'     '}'     '}'
T               'T'     '8'     't'     'T'     0x9     0x9
U               'U'     '8'     'u'     'U'     '5'     '5'
V               'V'     '8'     'v'     'V'     '^'     '^'
W               'W'     '9'     'w'     'W'     '~'     '~'
X               'X'     '9'     'x'     'X'     ']'     ']'
Y               'Y'     '9'     'y'     'Y'     '4'     '4'
